@model IEnumerable<CaresoftHMISDataAccess.HRPerformanceVariable>


<table class="table table-data table-condensed">

    <tr>

        <th>
            @Html.DisplayNameFor(model => model.KPIId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KPIName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CompoundIndicator)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Title1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Title2)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Title3)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Title4)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Remarks)
        </th>
<tbody class="bg-white">
        @foreach (var item in Model.OrderByDescending(e => e.Id))
        {
    <tr>

        <td>
            @Html.DisplayFor(modelItem => item.KPIId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.KPIName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CompoundIndicator)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Title1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Title2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Title3)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Title4)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Remarks)
        </td>
    </tr>
        }
    </tbody>
</table>






