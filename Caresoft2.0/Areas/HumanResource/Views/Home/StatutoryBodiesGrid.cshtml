@model IEnumerable<CaresoftHMISDataAccess.HRStatutoryBody>


<table class="table table-data table-condensed">

    <tr>

        <th>
            @Html.DisplayNameFor(model => model.StatutoryBodyId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StatutoryBodyName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Remarks)
        </th>

<tbody class="bg-white">
        @foreach (var item in Model.OrderByDescending(e => e.Id))
        {
    <tr>

        <td>
            @Html.DisplayFor(modelItem => item.StatutoryBodyId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StatutoryBodyName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Remarks)
        </td>
    </tr>
        }
    </tbody>
</table>



