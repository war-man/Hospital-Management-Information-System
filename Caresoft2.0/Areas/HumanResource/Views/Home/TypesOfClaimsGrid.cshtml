@model IEnumerable<CaresoftHMISDataAccess.HRInsuranceCover>


<table class="table table-data table-condensed">

    <tr>
        <th>Edit</th>
        <th>Delete</th>
        <th>
            @Html.DisplayNameFor(model => model.ClaimId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ClaimDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CompanyToClaimFrom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Remarks)
        </th>
<tbody class="bg-white">
        @foreach (var item in Model.OrderByDescending(e => e.Id))
        {
        <tr>
            <td>Edit</td>
            <td>Delete</td>
            <td>
                @Html.DisplayFor(modelItem => item.ClaimId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ClaimDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CompanyToClaimFrom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Remarks)
            </td>
        </tr>
        }
    </tbody>
</table>








