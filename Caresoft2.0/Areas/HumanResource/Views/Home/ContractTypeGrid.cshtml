@model IEnumerable<CaresoftHMISDataAccess.HRContract>


<table class="table table-data table-condensed">

    <tr>
        <th>Edit</th>

        <th>Delete</th>

        <th>
            @Html.DisplayNameFor(model => model.ContractId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContractType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Remarks)
        </th>
<tbody class="bg-white">
        @foreach (var item in Model.OrderByDescending(e => e.Id))
        {
        <tr>
            <td>Edit</td>

            <td>Delete</td>

            <td>
                @Html.DisplayFor(modelItem => item.ContractId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ContractType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Remarks)
            </td>
        </tr>
        }
    </tbody>
</table>



