@model IEnumerable<CaresoftHMISDataAccess.HREmploymentBenefit>


<table class="table table-data table-condensed">

    <tr>

        <th>
            @Html.DisplayNameFor(model => model.BenefitId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BenafitTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Departments)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Remarks)
        </th>
<tbody class="bg-white">
        @foreach (var item in Model.OrderByDescending(e => e.Id))
        {
    <tr>

        <td>
            @Html.DisplayFor(modelItem => item.BenefitId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BenafitTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Departments)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Remarks)
        </td>
    </tr>
        }
    </tbody>
</table>








