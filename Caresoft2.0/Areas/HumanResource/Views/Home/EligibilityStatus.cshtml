
@model IEnumerable<CaresoftHMISDataAccess.HRInsuranceCover>
@{

    ViewBag.Title = "Eligibility Status";
    ViewBag.showTopMenu = true;
    ViewBag.WideView = true;
    Layout = "~/Areas/HumanResource/Views/Shared/_Layout.cshtml";
}
<div class="col-md-2">
    @Html.Partial("~/Areas/HumanResource/Views/_SideNav.cshtml")
</div>
<div class="col-md-8">
    <div class="">
        <div class="panel panel-primary shadow glass caresoft-panel">
            <div class="panel-heading custom-navbar">
                <p>@ViewBag.Title</p>
            </div>
            <div class="panel-body">
                <div class="col-sm-12">

                    <form id="country" method="post" action="@Url.Action("SaveEligibilityStatusData","Home",new {Area="HumanResource"})">

                        <table class="form-class">
                            <tr>
                                <td><label>Eligibility Id</label></td>
                                <td>
                                    <input type="number" name="EligibilityId" class="text-box box-lg " />
                                </td>

                                <td><label>Eligibility Status</label></td>
                                <td>
                                    <input type="text" name="EligibilityStatus" class="text-box box-lg " />
                                </td>

                            </tr>

                        </table>
                        
                            <br />
                            <br />
                            <input id="Button1" class="btn btn-sm btn-primary" type="submit" style="margin-right:100px" value="Save" />
                        
                    </form>
                    <div id="ipd-list" class="scrollable-table-container">
                        @Html.Partial("EligibilityGrid")
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>











