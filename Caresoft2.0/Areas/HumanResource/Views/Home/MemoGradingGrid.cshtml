@model IEnumerable<CaresoftHMISDataAccess.HRGradingMemo>


<table class="table table-data table-condensed">

    <tr>
        <th>Edit</th>
        <th>Delete</th>
        <th>
            @Html.DisplayNameFor(model => model.MemoSource)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Grade)
        </th>
       
    <tbody class="bg-white">
        @foreach (var item in Model.OrderByDescending(e => e.Id))
        {
            <tr>
                <td>Edit</td>
                <td>Delete</td>
                <td>
                    @Html.DisplayFor(modelItem => item.MemoSource)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Grade)
                </td>
                
            </tr>
        }
    </tbody>
</table>







