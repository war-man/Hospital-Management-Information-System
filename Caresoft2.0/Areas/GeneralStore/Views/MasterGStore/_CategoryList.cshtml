@model IEnumerable<Caresoft2._0.Areas.GeneralStore.Models.CategoryGstore>

<table class="table table-data">
    <tr>
        <th>Edit</th>
        <th>Category</th>
        <th>Delete</th>
    </tr>
    @if (Model.Count() == 0)
    {
        <tr>
            <td colspan="3">
                No data to display
            </td>
        </tr>
    }
    else
    {
        foreach (var item in Model)
        {
            <tr>
                <td><i class="fa fa-edit"></i></td>
                <td>@item.CategoryName</td>
                <td><i class="fa fa-trash"></i></td>
            </tr>
        }

    }
</table>
