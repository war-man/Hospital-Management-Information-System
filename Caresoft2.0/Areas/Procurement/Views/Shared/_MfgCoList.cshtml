@model IEnumerable<Caresoft2._0.Areas.Procurement.Models.MfgCo>
    <div class="col-md-12">
        <table class="table table-data-pathology table-condensed table-hover">
            <tr>
                <th>Edit</th>
                <th>Manufacture Company Name</th>
                <th>Delete</th>
            </tr>
            <tbody>
                @if (Model != null || Model.Count() != 0)
                {

                    foreach (var item in Model)
                    {
                        <tr>
                            <td><button  class="btn btn-primary btn-xs btnEdit"  data-mfgco="@item.Id" >Edit</button></td>
                            <td>@item.Name</td>
                            <td><button class="btn btn-primary btn-xs btnDelete"  data-mfgco="@item.Id">Delete</button></td>
                        </tr>
                    }

                }
                else
                {
                    <tr><td colspan="12"> No Data to Display</td></tr>
                }
            </tbody>

        </table>
    </div>



