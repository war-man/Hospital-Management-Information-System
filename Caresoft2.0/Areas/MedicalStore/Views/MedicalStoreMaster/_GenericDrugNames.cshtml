@model  IEnumerable<Caresoft2._0.Areas.Procurement.Models.GenericDrugName>

<table class="table table-data-pathology table-condensed" style="background:white;">
    <tr>
        <th>Edit</th>
        <th>Generic Drug Name</th>
        <th>Delete</th>
    </tr>
    @if (Model == null || Model.Count() == 0)
    {
        <tr>
            <td colspan="3" class="text-center"> No Data to display</td>
        </tr>
    }
    else
    {
        foreach (var item in Model)
        {
        <tr>
            <td> <a href="#" class="btn btn-xs btn-primary">Edit</a></td>
            <td>
                @item.Name
            </td>
            <td> <a href="#" class="btn btn-xs btn-primary">Delete</a></td>

        </tr>
        }
    }

</table>