
@model IEnumerable<Caresoft2._0.Areas.Procurement.Models.ItemMaster>

<table class="table table-data-pathology table-condensed " id="tblEmployee" style="background:white; margin-top:10px;">
    <thead>
        <tr>
            <th>Edit</th>
            <th>Item Name</th>
            <th>Rack Name</th>
            <th>Cell in Rack</th>
            <th>Delete</th>

        </tr>
    </thead>
    <tbody>

        @if (Model.Count() < 1)
        {
            <tr>
                <td colspan="5" style="text-align:center;font-style:italic">
                    No Data to Display
                </td>
            </tr>
        }
        else
        {
            foreach (var item in Model)
            {
                <tr>
                    <td><button class="btn btn-primary btn-xs">Edit</button></td>
                    <td>@item.ItemName</td>
                    <td>@item.RackName</td>
                    <td>@item.CellInRack</td>
                    <td><button class="btn btn-primary btn-xs">Delete</button></td>
                </tr>
            }

        }

    </tbody>
</table>

