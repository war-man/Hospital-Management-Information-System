        function GetBatchNumbers(name) {
            var itemMasterData = null;
            var SelectedBatchNo = null;
            if (name.length > 1) {
                $('.loader').show();
                console.log(name);

                var url = "/PharmacyPatientTransaction/SearchItemSortingWithBatch?name=" + name;
                $.ajax({
                    type: "GET",
