                        }
                        else {
                            $("#BatchNo").val(obj.BatchNo);
                            $("#Rate").val(obj.SellingPrice);
                            $("#CurrentStock").val(obj.CurrentStock);
                            $("#ItemMasterId").val(obj.Id);
