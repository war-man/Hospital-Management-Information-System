
@{
    ViewBag.Title = "ItemLocationDetails";
    Layout = "~/Areas/FixedAssets/Views/Shared/_LayoutFixedAssets.cshtml";
}

@model IEnumerable<Caresoft2._0.Areas.Procurement.Models.Drug>


<div class="col-md-8">
    <div class="panel panel-primary caresoft-panel" style="margin-top:10px">
        <div class="panel-heading custom-navbar">
            <p style="color:black ; margin-left:5px"> <strong>Item Location Details</strong></p>
        </div>
        <div class="panel-body">
            <div class="row">
                <div class="col-md-12">
                    <table class="table table-data">
                        <tr>
                            <th>
                                Item Name
                            </th>
                            <th>
                                Rack Name
                            </th>
                            <th>
                                Cell ID
                            </th>
                        </tr>
                        @if (Model == null)
                        {
                            <tr>
                                <td colspan="3">
                                    No data to display
                                </td>
                            </tr>
                        }
                        else
                        {
                            foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @item.Name
                                    </td>
                                    <td>
                                        @item.Rack
                                    </td>
                                    <td>
                                        @item.CellinRack
                                    </td>
                                </tr>
                            }

                        }
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>





