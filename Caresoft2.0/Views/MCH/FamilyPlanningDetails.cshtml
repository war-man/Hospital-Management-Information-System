@model IEnumerable<CaresoftHMISDataAccess.MCHFamilyPlaning>


@{

}

<table class="table table-caresoft table-condensed">
    <thead>
        <tr>
            <td style="color:fuchsia">OPD No</td>
            <td>&nbsp;</td>
            <td>&nbsp;</td>
          


            <th style="color:fuchsia">User</th>
            <td>&nbsp;</td>


            <th style="color:fuchsia; font-family:Calibri; font:16px">Date Of Visit</th>


        </tr>
    </thead>
    <tbody class="bg-white">

        @foreach (var a in Model.OrderByDescending(a => a.Id))
        {
            <tr>
                <td>@a.OPDNo</td>
                <td>&nbsp;</td>
                <td>&nbsp;</td>
                

                <td>@a.UserId</td>
                <td>&nbsp;</td>
               
                
                <td style="color:crimson">@a.AddedOn</td>


            </tr>
        }

    </tbody>

    <td>&nbsp;</td>
    <thead>
        <tr>

            <th>Date</th>
            <th>Contraceptive</th>
            <th>Notes</th>
            <th>Next visit</th>
            <th>User</th>
            <th>Date Added</th>
        </tr>
    </thead>
    <tbody class="bg-white">

        @foreach (var a in Model.OrderByDescending(a => a.Id))
        {
            <tr style="background-color:chartreuse;">
                <td>@a.DateAdministered</td>
                <td>@a.ContraceptiveAdministered</td>
                <td>@a.ClinicalNotes</td>
                <td>@a.NextVisit</td>
                <td>@a.UserId</td>
                <td>@a.AddedOn</td>

            </tr>
        }

    </tbody>
</table>
    
