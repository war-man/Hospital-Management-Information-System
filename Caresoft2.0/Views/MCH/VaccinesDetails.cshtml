@model IEnumerable<CaresoftHMISDataAccess.MCHPreventativeService>


@{

}


<table class="table table-caresoft table-condensed">
    <thead>
        <tr>
            <td style="color:fuchsia">OPD No</td>
            <td>&nbsp;</td>
            <td>&nbsp;</td>
            <td>&nbsp;</td>


            <th style="color:fuchsia">User</th>
            <td>&nbsp;</td>
            <td>&nbsp;</td>
            <td>&nbsp;</td>

            <th style="color:fuchsia; font-family:Calibri; font:16px">Date Of Visit</th>


        </tr>
    </thead>
    <tbody class="bg-white">

        @foreach (var a in Model.OrderByDescending(a => a.Id))
        {
            <tr>
                <td>@a.OPDNo</td>
                <td>&nbsp;</td>
                <td>&nbsp;</td>
                <td>&nbsp;</td>

                <td>@a.UserId</td>
                <td>&nbsp;</td>
                <td>&nbsp;</td>
                <td>&nbsp;</td>
                <td style="color:crimson">@a.DateAdded</td>


            </tr>
        }

    </tbody>

    <td>&nbsp;</td>
    <thead>
        <tr>
            <td>&nbsp;</td>
            <th>Vaccine</th>
            <th>Date Given</th>
            <th>Next Visit</th>
            <th>1st Ferrous Fumarate</th>
            <th>2nd Ferrous Fumarate</th>
            <th>3rd Ferrous Fumarate</th>
            <th>4th Ferrous Fumarate</th>
            <td>&nbsp;</td>


        </tr>
    </thead>
    <tbody class="bg-white">

        @foreach (var a in Model.OrderByDescending(a => a.Id))
        {
            <tr style="background-color:chartreuse">
                <td>&nbsp;</td>
                <td>@a.Vaccine</td>
                <td>@a.DateGiven</td>
                <td>@a.NextVisit</td>
                <td>@a.FirstFerrousFumarate</td>
                <td>@a.SecondFerrousFumarate</td>
                <td>@a.ThirdFerrousFumarate</td>
                <td>@a.ForthFerrousFumarate</td>
                <td>&nbsp;</td>



            </tr>
        }

    </tbody>

</table>
   

