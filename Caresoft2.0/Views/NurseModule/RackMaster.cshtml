
@{
    ViewBag.Title = "Rack Master";
    Layout = "~/Views/Shared/_NurseModuleLayout.cshtml";
}
<div class="row">
    <div class="col-sm-6 col-sm-offset-3">
        <div class="panel panel-primary caresoft-panel">
            <div class="panel-heading">
                @ViewBag.Title
            </div>
            <div class="panel-body">
                <table class="table-form">
                    <tr>
                        <td>
                            <label>Rack Name</label>
                        </td>
                        <td>
                            <input type="text" class="text-box " placeholder="Voucher No"/>
                        </td>
                       
                    </tr>
                    <tr>
                        <td>
                            <label>Total Number Of Cells</label>
                        </td>
                        <td>
                            <input type="text" class="text-box "  />
                        </td>
                    
                    </tr>
                    <tr>
                        <td colspan="2">
                            <input type="button" class="btn btn-primary bnt-sm" value="Save" />
                           
                            <input type="button" class="btn btn-primary bnt-sm" value="Clear" />
                        </td>
                    </tr>
                </table>
                <table class="table-data table table-condensed">
                    <thead>
                        <tr>
                            <th>Edit</th>
                            <th>Rac Name</th>
                            <th>No Of Cells</th>
                            <th>Delete</th>
                        </tr>
                    </thead>
                    <tbody class="bg-white">
                        <tr>
                            <td colspan="4" align="center">
                                <label>No data to display</label>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>